{
  "description": "Polymer mixin for localization of D2L components",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightspaceUI/localize-behavior.git"
  },
  "name": "d2l-localize-behavior",
  "scripts": {
    "lint:js": "eslint . --ext .js,.html test/**/*.js demo/**/*.html demo/**/*.js",
    "lint": "npm run lint:js",
    "start": "web-dev-server --node-resolve --watch --open --app-index demo/index.html",
    "test": "npm run lint && npm run test:headless",
    "test:headless": "web-test-runner --group default",
    "test:headless:watch": "web-test-runner --group default --watch"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/eslint-parser": "^7",
    "@brightspace-ui/core": "^2",
    "@open-wc/testing": "^3",
    "@web/dev-server": "^0.1",
    "@web/test-runner": "^0.13",
    "eslint": "^8",
    "eslint-config-brightspace": "^0.16",
    "eslint-plugin-html": "^6",
    "eslint-plugin-import": "^2",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "sinon": "^14"
  },
  "version": "2.5.10",
  "main": "d2l-localize-behavior.js",
  "dependencies": {
    "@brightspace-ui/intl": "^3",
    "@polymer/iron-ajax": "^3.0.0-pre.26",
    "@polymer/polymer": "^3",
    "intl-messageformat": "^7"
  }
}
